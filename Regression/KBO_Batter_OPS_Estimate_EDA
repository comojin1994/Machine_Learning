    "# 삭제할 데이터 추출\n",
    "drop_idx = regular_season_df.loc[((regular_season_df['H']>0)&(regular_season_df['SLG']==0)) |\n",
    "        # 안타가 0개 이상이면서 장타율이 0인 경우\n",
    "(((regular_season_df['H']>0)|(regular_season_df['BB']>0)|(regular_season_df['HBP']>0))\n",
    "# 안타가 0개 이상 혹은 볼넷이 0개 이상 혹은 몸에 맞은 볼이 0개 이상인 경우\n",
    "& (regular_season_df['OBP']==0))].index\n",
    "        # 출루율이 0인 경우 \n",
    "\n",
    "# 데이터 삭제\n",
    "regular_season_df = regular_season_df.drop(drop_idx).reset_index(drop=True)\n",
    "regular_season_df\n"
   ]
